%macro m_import_results(start,stop);
%do year=&start %to &stop %by 1;
data _null_;
	set bdlref.url;
	if year = &year then do;
		call symputx('url_ref',url);
		call symputx('id',league_id);
		end;
run;
%put &url_ref;
%put &id;
data _null_;
	set bdlref.xml_map;
	if program = 'weeklyResults' then do;
			call symputx('map',map_file);
			end;
run;
%put &map;
data _null_;
	call symputx('Url',CATX('.',"http://www&url_ref","myfantasyleague","com/&year/export?TYPE=weeklyResults&L=&id&W=YTD&JSON=0"));
run;
%put &url;
filename  output URL "&Url";
filename  SXLEMAP "/home/cwillis/BDL/data/xml_maps/&map";
libname   output xmlv2 xmlmap=SXLEMAP access=READONLY;
DATA Franchise_&year; 
	SET output.franchise;
		year="&year";
run;
DATA player_&year; 
	SET output.player;
		year="&year";
run;
DATA player1_&year; 
	SET output.player1;
		year="&year";
run;
DATA franchise1_&year; 
	SET output.franchise1;
		year="&year";
run;
DATA Matchup_&year; 
	SET output.matchup;
		year="&year";
run;
%end;
%mend m_import_results;
